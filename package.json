{
  "name": "custom-server-express",
  "version": "1.0.0",
  "scripts": {
    "devxx": "ts-node-dev --respawn --transpileOnly ./server.ts",
    "buildxx": "next build",
    "startxx": "cross-env NODE_ENV=production node server.js",
    "tsc": "tsc",
    "dev": "nodemon --exec ts-node ./server.ts",
    "prod": "tsc && node ./build/app.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@types/express": "^4.17.3",
    "@types/node": "^12.12.31",
    "@types/react": "^16.9.25",
    "cross-env": "^5.2.0",
    "express": "^4.14.0",
    "next": "latest",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.1"
  }
}
